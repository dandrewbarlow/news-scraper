{
  "websites" :
  [
    {
      "name": "BBC",
      "url" : "https://bbc.com/news",
      "regex" : "heading",
      "element" : "h3"
    },

    {
      "name" : "WSJ",
      "url" : "https://www.wsj.com/",
      "regex" : "WSJTheme--headline",
      "element" : "h3"
    },

    {
      "name" : "NYT",
      "url" : "https://nytimes.com",
      "regex" : "",
      "element" : "h2"
    },

    {
      "name": "CNN",
      "url" : "https://cnn.com",
      "regex" : "(headline|banner-text)",
      "element" : ["h2", "strong", "span"]
    },

    {
      "name": "NBC",
      "url" : "https://nbcnews.com",
      "regex" : "headline",
      "element" : ["span", "h3"]
    },

    {
      "name": "Yahoo",
      "url" : "https://news.yahoo.com",
      "regex" : "",
      "element" : ["h2", "h3"]
    },

    {
      "name": "NPR",
      "url" : "https://npr.org",
      "regex" : "title",
      "element" : "h3"
    },

    {
      "name": "Huffington Post",
      "url" : "https://www.huffpost.com/",
      "regex" : "headline",
      "element" : ["a", "h2"]
    }
  ],

  "options" :
  {
    "outfile" : "headlines",
    "filetype" : "csv"
  }
}
